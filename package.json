  },
  "keywords": [
    "cli",
    "user management",
    "typescript",
    "swc",
    "tsx",
    "command-line",
    "nodejs",
    "developer tools",
    "user manager"
  ],
  "author": "Sobhan Yaghoubi",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/sobhan-yaghoubi/user-management-cli.git"
  },
  "type": "module",
  "dependencies": {
    "@prisma/client": "^6.5.0",
    "bcryptjs": "^3.0.2",
    "commander": "^13.1.0",
    "inquirer": "^12.5.0",
    "tsx": "^4.19.3"
  },
  "devDependencies": {
    "@swc/cli": "^0.6.0",
    "@swc/core": "^1.11.13",
    "@types/figlet": "^1.7.0",
    "@types/node": "^22.13.12",
    "prisma": "^6.5.0",
    "typescript": "^5.8.2"
